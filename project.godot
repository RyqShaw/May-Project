[application]

config/name="MayProject"
run/main_scene="res://MainMenu/MainMenu.tscn"
boot_splash/image="res://MainMenu/bootsplash.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"

[autoload]

SoundManager="*res://addons/sound_manager/sound_manager.tscn"
GlobalInfo="*res://GlobalInfo.gd"
Save="*res://save.gd"

[debug]

gdscript/warnings/return_value_discarded=false

[display]

window/size/width=1280
window/size/height=720
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/sound_manager/plugin.cfg" )

[global]

